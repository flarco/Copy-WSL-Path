{
    "name": "copy-wsl-path-fl",
    "displayName": "Copy WSL Path [FL]",
    "description": "Add Copy WSL Path to the file context menu",
    "version": "0.0.2",
    "publisher": "CoenraadSFL",
    "engines": {
        "vscode": "^1.18.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "copy-WSL-path.copy",
                "title": "Copy WSL Path"
            }
        ],
        "menus": {
            "editor/title/context": [
                {
                    "command": "copy-WSL-path.copy",
                    "group": "copy-WSL-path"
                }
            ],
            "explorer/context": [
                {
                    "command": "copy-WSL-path.copy",
                    "group": "copy-WSL-path"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "copy-paste": "^1.3.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/CoenraadS/Copy-WSL-Path.git"
    },
    "license": "SEE LICENSE IN LICENSE.md"
}
